<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <dependency>
        <groupId>javax.mail</groupId>
        <artifactId>javax.mail-api</artifactId>
        <version>1.6.2</version>
    </dependency>
    
    <servlet>
        <servlet-name>UserController</servlet-name>
        <servlet-class>Controllers.UserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteStudentServlet</servlet-name>
        <servlet-class>Controllers.DeleteStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditStudentServlet</servlet-name>
        <servlet-class>Controllers.EditStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentScheduleServlet</servlet-name>
        <servlet-class>Controllers.StudentScheduleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>courseRegistrationServlet</servlet-name>
        <servlet-class>Controllers.courseRegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CourseRegistrationServlet</servlet-name>
        <servlet-class>Controllers.CourseRegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EachRegisterCourseServlet</servlet-name>
        <servlet-class>Controllers.EachRegisterCourseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewRegisteredCoursesServlet</servlet-name>
        <servlet-class>Controllers.ViewRegisteredCoursesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>Controllers.AdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterControl</servlet-name>
        <servlet-class>Controllers.RegisterControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveCourseServlet</servlet-name>
        <servlet-class>Controllers.RemoveCourseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GoBackHomeServlet</servlet-name>
        <servlet-class>Controllers.GoBackHomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyEmailServlet</servlet-name>
        <servlet-class>Controllers.VerifyEmailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserController</servlet-name>
        <url-pattern>/UserController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteStudentServlet</servlet-name>
        <url-pattern>/DeleteStudentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditStudentServlet</servlet-name>
        <url-pattern>/EditStudentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentScheduleServlet</servlet-name>
        <url-pattern>/StudentScheduleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CourseRegistrationServlet</servlet-name>
        <url-pattern>/CourseRegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EachRegisterCourseServlet</servlet-name>
        <url-pattern>/EachRegisterCourseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewRegisteredCoursesServlet</servlet-name>
        <url-pattern>/ViewRegisteredCoursesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterControl</servlet-name>
        <url-pattern>/RegisterControl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveCourseServlet</servlet-name>
        <url-pattern>/RemoveCourseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GoBackHomeServlet</servlet-name>
        <url-pattern>/GoBackHomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyEmailServlet</servlet-name>
        <url-pattern>/VerifyEmailServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
